<%= form_for :mach, url: { controller: "home", action: "update", id: @mach }, html: { method: :put } do |f| %>
	<p>
	IP: <strong><%= @ip.ip_addr %></strong>
	</p>
	<p>
	<%= f.label "Process" %>
	<%= f.text_field :server %>
	</p>
	<p>
	<%= f.label "Port" %>
	<%= f.text_field :port %>
	</p>
	<%= f.submit "Update" %>
<% end %>
